{
  "extends": ["taro/react"],
  "ignorePatterns": ["babel.config.js", "postcss.config.js"],
  "rules": {
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "@typescript-eslint/no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "no-unused-vars": "off",
    "jsx-quotes": "off",
  },
}
